{
  "name": "conta-azul-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "chokidar": "^1.7.0",
    "react-scripts": "1.0.7",
    "stylus": "^0.54.5",
    "stylint": "^1.5.9"
  },
  "scripts": {
    "build-css": "stylus src/css/base.styl -o src/index.css",
    "watch-css": "npm run build-css && chokidar 'src/**/*.styl' 'npm run build-css'",
    "lint-css": "stylint src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run lint-css && react-scripts test --env=jsdom",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  }
}
